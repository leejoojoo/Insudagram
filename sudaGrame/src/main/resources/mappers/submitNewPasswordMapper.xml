<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.suda.mapper.SubmitNewPasswordMapper">

	<update id="updateMyPassword" parameterType="org.sudaPro.web.myPage.vo.MypageVO">
	
		update member set m_password = #{m_password}
		where m_code = #{m_code}

	</update>

</mapper>